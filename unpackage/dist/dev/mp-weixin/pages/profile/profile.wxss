
.header{
	width: 100%;
	height: 160rpx;
	background-color: #ff8198;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.yuan{
	width: 120rpx;
	height: 120rpx;
	border-radius: 50%;
	background-color: #FFFFFF;
	margin-left: 40rpx;
	margin-top: 20rpx;
}
.txt{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	color: #FFFFFF;
	margin-top: 40rpx;
}
.txt1{
	font-size: 32rpx;
	padding-left: 20rpx;
}
.txt2{
	font-size: 28rpx;
	padding-left: 30rpx
}
.content{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 180rpx;
	background-color: rgb(215, 215, 215,0.4);
	-webkit-justify-content: space-around;
	        justify-content: space-around;
}
.content>view{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	height: 160rpx;
	width: 30%;
	background-color: #FFFFFF;
	text-align: center;
}
.a{
	color: #FF5F3E;
	font-size: 20px;
	line-height: 90rpx;
}
.content>view view{
	font-size: 24rpx;
}
.foot{
	width: 100%;
	height: 280rpx;
	background-color: rgb(215, 215, 215,0.4);
}
.zong{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 88rpx;
}
.null{
	width: 10%;
	height: 88rpx;
	background-color: #FFFFFF;
}
.row{
	width: 90%;
	height: 88rpx;
	line-height: 88rpx;
	background-color: #FFFFFF;
	font-size: 28rpx;
}

